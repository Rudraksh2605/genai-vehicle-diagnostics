{
  "signals": [
    {
      "id": "speed",
      "name": "Vehicle Speed",
      "unit": "km/h",
      "min": 0,
      "max": 200,
      "normal_range": [0, 120],
      "ui_widget": "speed_gauge",
      "analytics_rules": [
        {
          "condition": "value > 100",
          "sustained_seconds": 10,
          "alert_id": "high_speed_stress",
          "severity": "warning",
          "message": "High Speed Stress Warning: Vehicle sustained speed above 100 km/h"
        }
      ]
    },
    {
      "id": "battery_soc",
      "name": "Battery State of Charge",
      "unit": "%",
      "min": 0,
      "max": 100,
      "normal_range": [20, 100],
      "ui_widget": "battery_indicator",
      "analytics_rules": [
        {
          "condition": "rapid_drop > 5",
          "window_seconds": 30,
          "alert_id": "battery_degradation",
          "severity": "critical",
          "message": "Battery Degradation Alert: SoC dropped more than 5% rapidly"
        }
      ]
    },
    {
      "id": "tire_pressure_fl",
      "name": "Tire Pressure - Front Left",
      "unit": "PSI",
      "min": 0,
      "max": 50,
      "normal_range": [28, 35],
      "ui_widget": "tire_pressure_card",
      "analytics_rules": [
        {
          "condition": "value < 25",
          "alert_id": "tire_pressure_low_fl",
          "severity": "critical",
          "message": "Possible Tire Failure: Front Left tire pressure critically low"
        }
      ]
    },
    {
      "id": "tire_pressure_fr",
